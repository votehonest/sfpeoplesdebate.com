generator client {
  provider   = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Question {
  id        String   @id @unique @default(uuid())
  createdAt DateTime @default(now())

  question String
  name     String?
  token    String  @unique @default(uuid())
}
